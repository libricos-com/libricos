{
    "_comments": [{
        "versioning": "https://getcomposer.org/doc/articles/versions.md",
        "sources": "https://wpackagist.org/",
        "wpackagist-plugin/aawp": ">=3.14.2"
    }],
    "name": "jesuserro/wordpress-site-about-books",
    "description": "Another WordPress site",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org",
            "only": ["wpackagist-plugin/*", "wpackagist-theme/*"]
        }
    ],
    "require": {
        "twbs/bootstrap": ">=4.5.3",

        "wpackagist-theme/twentyeleven":">=3.5",
        
        "wpackagist-plugin/all-in-one-wp-migration":">=7.30",
        "wpackagist-plugin/instagram-feed":">=2.5.4",
        "wpackagist-plugin/permalink-manager":">=2.2.9.2",
        "wpackagist-plugin/pods":">=2.7.24",
        "wpackagist-plugin/seo-by-rank-math":">=1.0.52",
        "wpackagist-plugin/updraftplus":">=1.16.36",
        "wpackagist-plugin/yet-another-related-posts-plugin":">=5.10.2"
    },
    "extra": {
        "installer-paths": {
            "wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
            "wp-content/themes/{$name}/": ["type:wordpress-theme"]
        }
    }
}
